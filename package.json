{
  "name": "rss-o-bot",
  "version": "1.0.0-rc.7",
  "description": "A FOSS RSS Bot alternative",
  "main": "dist",
  "bin": "dist/cli.js",
  "man": "dist/docs/rss-o-bot.1",
  "scripts": {
    "build": "npm run --silent build:js && ./dist/cli.js build-man --config=tests/config/succeeding",
    "build:js": "babel src --out-dir dist",
    "build:tests": "babel --presets babel-preset-es2015 tests/src --out-dir tests/dist",
    "build:deps-graph": "madge -i ./src/docs/dependency-graph.svg src",
    "watch:build:tests": "watch 'npm run --silent build:tests' tests/src",
    "watch:build": "watch 'npm run --silent build:js -- --source-maps' src",
    "watch:test": "RSS_O_BOT_TESTING_MODE=1 ava tests/src/*.js --watch",
    "test": "npm run --silent build:tests && RSS_O_BOT_TESTING_MODE=1 ava tests/dist/*/*.js",
    "test:debug": "DEBUG=rss-o-bot RSS_O_BOT_TESTING_MODE=1 ava",
    "test:ci": "npm run --silent build && npm run --silent test && eslint && npm run --silent test-coverage",
    "test:coverage": "nyc --include dist/ npm test && nyc report --reporter=text-lcov | coveralls",
    "ava": "RSS_O_BOT_TESTING_MODE=1 ava",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "prepublish": "npm run --silent build"
  },
  "repository": "https://github.com/Kriegslustig/rss-o-bot.git",
  "author": "Kriegslustig <npm@ls7.ch>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "cli-table2": "^0.2.0",
    "debug": "^2.2.0",
    "feedparser": "^1.1.4",
    "immutable": "^3.8.1",
    "json-schema": "^0.2.3",
    "jsonwebtoken": "^7.0.0",
    "marked-man": "^0.1.5",
    "moment": "^2.13.0",
    "node-uuid": "^1.4.7",
    "ramda": "^0.22.1",
    "rxjs": "^5.0.0-beta.12",
    "sax": "^1.2.1",
    "sequelize": "^3.23.3",
    "sqlite3": "^3.1.4",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.9.1",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "^6.9.0",
    "coveralls": "^2.11.9",
    "eslint": "^3.5.0",
    "eslint-config-standard": "^6.0.1",
    "eslint-plugin-flowtype": "^2.18.2",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.0",
    "flow-bin": "^0.33.0",
    "flow-typed": "^2.0.0-beta.7",
    "nyc": "^8.3.0",
    "watch": "^0.19.2"
  },
  "peerDependencies": {
    "rss-o-bot-telegram": "0.1.x",
    "rss-o-bot-desktop": "0.1.x",
    "rss-o-bot-email": "0.1.x"
  }
}
